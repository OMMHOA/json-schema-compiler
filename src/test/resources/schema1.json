{
  "title": "Example1",
  "description": "Defining an object with string and integer constraints.",
  "type": "object",
  "required": ["first_name", "last_name", "age"],
  "properties": {
    "first_name": {
      "type": "string",
      "pattern": "^[a-zA-Z' -]*$"
    },
    "last_name": {
      "type": "string",
      "pattern": "^[a-zA-Z' -]*$"
    },
    "age": {
      "type": "integer",
      "minimum": 18,
      "maximum": 65,
      "exclusiveMaximum": true
    },
    "nickname": {
      "type": "string",
      "minLength": 3,
      "maxLength": 10
    }
  }
}